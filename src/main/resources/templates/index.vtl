<h1>Urban Dictionary</h1>

<ul>
  #foreach($word in $words)
  <li><a href="/word/$word.getId()">$word.getWord()</a></li>
  #end
</ul>

<form class="form-group jumbotron" action="/" method="post">
  <div class="form-group">
    <label for="word"></label>
    <input id="word" clas="form-control" type="text" name="word" value="">
  </div>
  <button type="submit" class="btn btn-default"name="button">Add word to library</button>
</form>
